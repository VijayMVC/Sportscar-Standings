      <StylesheetText>
.summary{margin-bottom:10px}
.searchBox{
  margin-top:8px;float: right;
}
.allmake ul,
.allmake ul li{width:126px; float:left; padding:0; margin:0}
.allmake ul li{line-height:23px !important;}
.IE .allmake ul li{height:23px ;}
table.ContactUsForm{}
table.ContactUsForm td{
  margin: 0;
  padding: 6px 0 0 0;
  line-height: 10px !important;
  vertical-align: top;
  width: 86px;
}
table.ContactUsForm td div input,table.ContactUsForm td div textarea{border:1px solid #ccc;}
table.ContactUsForm td div input{
  width: 248px;
  line-height: 20px;
  margin-bottom: 9px;
  height: 21px;
}
table.ContactUsForm td div textarea{width:248px;resize:none}
.EditingFormLabel{float:left;padding-top:5px; width:95px}

.allmakefooter{margin:0 20%; width:670px;margin-top:10px}
.allmakefooter ul{background:#fff; border:0px solid #ccc; display:block;float: left;
width: 110px;min-height: 249px; margin:0 10px 15px 0 !important;}
.allmakefooter ul li{display:inline-table;padding:10px 10px 1px 10px; margin:0; min-width:90px;}
.allmakefooter ul li a{border:none}

.directory{margin-bottom:20px}

/*TimeClass Paging*/
.PagerNumberArea{float: right;
padding-top: 4px;
padding-right: 10px;}
.PagerResults{float: left;padding-left:10px}

.footernav{padding-bottom:8px;}
.footernav ul#menuElem{ width:344px; margin:0 auto}
.feature205{width:205px !important}
