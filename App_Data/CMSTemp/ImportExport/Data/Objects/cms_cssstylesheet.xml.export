.LTR, .RTL
{
  background-color: #ffffff;
}

body
{
  background-color: #ffffff;
  font-size: small;
  font-family: Arial;
  margin: 0px;
}

h1
{
  color: #ffa21e;
  font-size: 160%;
}

h2
{
  font-size: 130%
}

h3
{
  font-size: 120%
}

h4
{
  font-size: 110%
}

h5
{
  font-size: 100%
}

h2,h3,h4,h5
{
  color: #002756;
}

a
{
  text-decoration: underline;
  color: #000000;
}

a:hover
{
  text-decoration: none;
}

.ErrorMessage
{
  color: #ff0000;
