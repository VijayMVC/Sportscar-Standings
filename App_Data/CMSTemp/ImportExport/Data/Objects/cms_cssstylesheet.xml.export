      <TaskTitle>Delete CSS stylesheet 'Corporate Site - Mobile device'</TaskTitle>
      <TaskData>&lt;NewDataSet&gt;&lt;CMS_CssStyleSheet&gt;
  &lt;StylesheetID&gt;3&lt;/StylesheetID&gt;
  &lt;StylesheetDisplayName&gt;Corporate Site - Mobile device&lt;/StylesheetDisplayName&gt;
  &lt;StylesheetName&gt;CorporateSiteMobile&lt;/StylesheetName&gt;
  &lt;StylesheetText&gt;/*Mobile CSS Stylesheet*/

body
{
    padding: 0;
    margin: 0;
    font-family: Arial;
    font-size: 35px;
}

.clear
{
    height: 0;
    line-height: 0;
    font-size: 0pt;
    clear: both;
    margin: 0;
    padding: 0;
    width: 100%;
}

h1, h2
{
    font-size: 1.7em;
    color: #000000;
    margin: 15px 0 30px 0;
    font-weight: bold;
}

h2
{
    font-size: 1.4em;
    margin: 15px 0 20px 0;
}

p
{
    font-size: 1em;
    color: #737373;
    margin-bottom: 30px;
}

p strong
{
    color: #000000;
}

a
{
    text-decoration: none;
    color: #1C64A6;
}

/*#Header#*/
.logo
{
    width: 100%;
    height: 120px;
    border-bottom: 2px solid #a1b7cf;
    background: #1e4077 url(~/App_Themes/CorporateSite/Images/Mobile/bg_logo.png) repeat-x top left;
}

.logo .inner, .footer .inner, .navigation .inner, .search .inner
{
    padding: 0;
    margin: 0 auto;
    width: 95%;
}

.logo .inner
{
    padding-top: 15px;
}

.navigation
{
    width: 100%;
    border-bottom: 2px solid #758293;
    background: #aecae7 url(~/App_Themes/CorporateSite/Images/Mobile/bg_nav.png) repeat-x top left;
}

.search
{
    width: 100%;
    border-bottom: 2px solid #758293;
    background: #eeeeee url(~/App_Themes/CorporateSite/Images/Mobile/bg_search.png) repeat-x top left;
}

.search .inner .searchBox input[type=text]
{
    width: 70%;
    border: 2px solid #758293;
    font-size: 1.0em;
    padding: 15px 10px;
    margin: 10px 20px 10px 0;

}

.search .inner .searchBox input[type=submit]
{
    border: 2px solid #758293;
    background: #d6dee9  url(~/App_Themes/CorporateSite/Images/Mobile/bg_button.png) repeat-x top left;   
    color: #0378bf;
    font-weight: bold;
    position: relative;
    top: 0px;
    padding: 15px;
    font-size: 1.0em;
}

.Safari .search .inner .searchBox input[type=submit]
{
    top: 1px;
}

.search .inner h1
{
    margin: 0px;
    color: #52677e;
    font-size: 1.4em;
    padding-top: 30px;
    }

.gap
{
    width : 100%;
    height: 40px;
}

.CMSValidator
{
    color: Red;
}

/*#Navigation#*/

.navigation .inner ul
{
    list-style: none;
    margin: 0;
    padding: 0;
    font-size: 1.0em;
    font-weight: bold;
}

.navigation .inner li a
{
    float: left;
    padding: 15px 25px;
    margin: 10px 0px 10px 0;
    border: 2px solid transparent;
}

.navigation .inner li
{
   float: left;  
}

.navigation .inner .HighLighted a
{
    border: 2px solid #839bb5;
    background: #ffffff url(~/App_Themes/CorporateSite/Images/Mobile/bg_nav_active.png) repeat-x bottom left;
}

.navigation .inner li a
{
    text-decoration: none;
    color: #1c64a6;
}

/*#Content#*/

.content
{
    padding: 20px 0 0 0;
    margin: 0 auto;
    width: 95%;
    color: #737373;
    text-align: left;
}

.content .detail img
{
    border: 2px solid #c1c1c1 !important;
    margin: 0 20px 20px 0;
    float: left;
}

/*#Content/List#*/

.listPreview
{
    padding: 35px 15px 18px 20px;
    background: #eeeeee url(~/App_Themes/CorporateSite/Images/Mobile/bg_news.png) repeat-x top left; 
    border-bottom: 2px solid #d3d3d3; 
    color: #1c64a6; 
    font-weight: bold;
}

.block
{
    display: block;
}

.listPreview .desc
{
    float: left;
    font-size: 1em;   
}

.listPreview .desc .date
{
    color: #737373;
    font-size: 1em;
    font-weight: normal;  
}

.listPreview .arrow 
{
    background: url(~/App_Themes/CorporateSite/Images/Mobile/arrow.png) no-repeat 0 10px;
    width: 24px;
    height: 46px;
    float: right;
}

/*#Content/Back button#*/
 .backButton
{
    font-size: 1.0em;
    color: #52677e;
    font-weight: bold;
    text-decoration: none;
    background: url(~/App_Themes/CorporateSite/Images/Mobile/bg_nav_active.png) repeat-x bottom left;
    border: 2px solid #839bb5;
    padding: 15px;
    margin: 21px 30px 0 0;
    float: left;
}

/*#Content/Search results#*/

.searchResult
{
    background: #eeeeee url(~/App_Themes/CorporateSite/Images/Mobile/bg_news.png) repeat-x top left;
    border-bottom: 2px solid #d3d3d3; 
    padding: 20px;
}

.searchResult h1
{
    margin-bottom: 10px;
}

.searchResult span
{
    color: #1c64a6;
}

.searchResult .date
{
    color: #737373;
}

.searchResult .keyword
{
    background: #d5e8fb;
}

/*#Footer#*/

.footer
{
    width: 100%;
    border-top: 2px solid #bfbfbf;
    background: #e6e6e6;
    margin-top: 40px;
}

.footer .inner .powered
{
    float: right;
    margin: 20px 0;
}

/*#Pager#*/

.pager
{
    padding-top: 50px;
}

.CurrentPage
{
    background: #ffffff;
    border: 2px solid #839bb5;
    color: black;
    padding: 20px 35px;
    float: left;
    margin-right: 40px;
    font-weight: bold;
    font-size: 1.0em;
}

.Pages
{
    background: #ffffff url(~/App_Themes/CorporateSite/Images/Mobile/bg_pager.png) repeat-x bottom left;
    color: black;
    padding: 20px 0;
    float: left;
    margin-right: 40px;
    font-size: 1.0em;
    font-weight: bold;
    border: 2px solid #839bb5;
}

.Pages a
{
    text-decoration: none;
    color: #000000;
    padding: 20px 35px;
}

.PreviousPage, .NextPage
{
  background: url(~/App_Themes/CorporateSite/Images/Mobile/bg_pager_pp.png) no-repeat 0px 0px ;
  width: 57px;
  height: 86px;
  display: block;
  float: left;
  margin-right: 40px;
}

.NextPage
{
  background: url(~/App_Themes/CorporateSite/Images/Mobile/bg_pager_np.png) no-repeat 0px 0px;
}


/*#Design mode#*/

.DesignMode .PagePlaceholder .logo
{
    height: auto;
}

.DesignMode .PagePlaceholder .navigation
{
    height: auto;
}

.DesignMode .PagePlaceholder .search
{
    height: auto;
}

.DesignMode .PagePlaceholder .footer
{
    height: auto;
}


