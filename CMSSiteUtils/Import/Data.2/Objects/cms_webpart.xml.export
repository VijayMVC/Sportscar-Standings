      <WebPartFileName>Viewers/DateTime.ascx</WebPartFileName>
      <WebPartProperties>&lt;form&gt;&lt;field column="JsUseServerTime" fieldcaption="Use server time" visible="true" defaultvalue="false" columntype="boolean" fieldtype="CustomUserControl" allowempty="false" isPK="false" system="false" fielddescription="Indicates if the server time should be used." publicfield="false" spellcheck="true" guid="29ba7b1a-c0a1-4ccb-a45f-cc848e8196aa" visibility="none" ismacro="false" hasdependingfields="false" dependsonanotherfield="false"&gt;&lt;settings&gt;&lt;controlname&gt;checkboxcontrol&lt;/controlname&gt;&lt;/settings&gt;&lt;/field&gt;&lt;field column="JsFormat" fieldcaption="Date and time format" visible="true" defaultvalue="dd.mm.yy HH:MM:ss" columntype="text" fieldtype="CustomUserControl" allowempty="false" isPK="false" system="false" columnsize="50" fielddescription="Sets the format of the displayed date and time using standard specifiers. Example: dd.mm.yy HH:MM:ss" publicfield="false" spellcheck="true" guid="3d309197-b155-4205-910b-bc92d834d208" visibility="none" ismacro="false" hasdependingfields="false" dependsonanotherfield="false"&gt;&lt;settings&gt;&lt;Trim&gt;False&lt;/Trim&gt;&lt;AutoCompleteFirstRowSelected&gt;False&lt;/AutoCompleteFirstRowSelected&gt;&lt;AutoCompleteShowOnlyCurrentWordInCompletionListItem&gt;False&lt;/AutoCompleteShowOnlyCurrentWordInCompletionListItem&gt;&lt;FilterMode&gt;False&lt;/FilterMode&gt;&lt;AutoCompleteEnableCaching&gt;False&lt;/AutoCompleteEnableCaching&gt;&lt;controlname&gt;textboxcontrol&lt;/controlname&gt;&lt;/settings&gt;&lt;/field&gt;&lt;/form&gt;</WebPartProperties>
      <WebPartCategoryID>184</WebPartCategoryID>
      <WebPartDocumentation>&lt;table cellspacing="0" summary="Date Format mask metasequences"&gt;
 &lt;thead&gt;
  &lt;tr&gt;
   &lt;th&gt;
    Mask&lt;/th&gt;
   &lt;th style="text-align: left"&gt;
    Description&lt;/th&gt;
  &lt;/tr&gt;
 &lt;/thead&gt;
 &lt;tbody&gt;
  &lt;tr&gt;
   &lt;td&gt;
    d&lt;/td&gt;
   &lt;td&gt;
    Day of the month as digits; no leading zero for single-digit days.&lt;/td&gt;
  &lt;/tr&gt;
  &lt;tr class="altBg"&gt;
   &lt;td&gt;
    dd&lt;/td&gt;
   &lt;td&gt;
    Day of the month as digits; leading zero for single-digit days.&lt;/td&gt;
  &lt;/tr&gt;
  &lt;tr&gt;
   &lt;td&gt;
    ddd&lt;/td&gt;
   &lt;td&gt;
    Day of the week as a three-letter abbreviation.&lt;/td&gt;
  &lt;/tr&gt;
  &lt;tr class="altBg"&gt;
   &lt;td&gt;
    dddd&lt;/td&gt;
   &lt;td&gt;
    Day of the week as its full name.&lt;/td&gt;
  &lt;/tr&gt;
  &lt;tr&gt;
   &lt;td&gt;
    m&lt;/td&gt;
   &lt;td&gt;
    Month as digits; no leading zero for single-digit months.&lt;/td&gt;
  &lt;/tr&gt;
  &lt;tr class="altBg"&gt;
   &lt;td&gt;
    mm&lt;/td&gt;
   &lt;td&gt;
    Month as digits; leading zero for single-digit months.&lt;/td&gt;
  &lt;/tr&gt;
  &lt;tr&gt;
   &lt;td&gt;
    mmm&lt;/td&gt;
   &lt;td&gt;
    Month as a three-letter abbreviation.&lt;/td&gt;
  &lt;/tr&gt;
  &lt;tr class="altBg"&gt;
   &lt;td&gt;
    mmmm&lt;/td&gt;
   &lt;td&gt;
    Month as its full name.&lt;/td&gt;
  &lt;/tr&gt;
  &lt;tr&gt;
   &lt;td&gt;
    yy&lt;/td&gt;
   &lt;td&gt;
    Year as last two digits; leading zero for years less than 10.&lt;/td&gt;
  &lt;/tr&gt;
  &lt;tr class="altBg"&gt;
   &lt;td&gt;
    yyyy&lt;/td&gt;
   &lt;td&gt;
    Year represented by four digits.&lt;/td&gt;
  &lt;/tr&gt;
  &lt;tr&gt;
   &lt;td&gt;
    h&lt;/td&gt;
   &lt;td&gt;
    Hours; no leading zero for single-digit hours (12-hour clock).&lt;/td&gt;
  &lt;/tr&gt;
  &lt;tr class="altBg"&gt;
   &lt;td&gt;
    hh&lt;/td&gt;
   &lt;td&gt;
    Hours; leading zero for single-digit hours (12-hour clock).&lt;/td&gt;
  &lt;/tr&gt;
  &lt;tr&gt;
   &lt;td&gt;
    H&lt;/td&gt;
   &lt;td&gt;
    Hours; no leading zero for single-digit hours (24-hour clock).&lt;/td&gt;
  &lt;/tr&gt;
  &lt;tr class="altBg"&gt;
   &lt;td&gt;
    HH&lt;/td&gt;
   &lt;td&gt;
    Hours; leading zero for single-digit hours (24-hour clock).&lt;/td&gt;
  &lt;/tr&gt;
  &lt;tr&gt;
   &lt;td&gt;
    M&lt;/td&gt;
   &lt;td&gt;
    Minutes; no leading zero for single-digit minutes.&lt;br /&gt;
    &lt;span class="small"&gt;Uppercase M unlike CF &lt;code&gt;timeFormat&lt;/code&gt;&amp;#39;s m to avoid conflict with months.&lt;/span&gt;&lt;/td&gt;
  &lt;/tr&gt;
  &lt;tr class="altBg"&gt;
   &lt;td&gt;
    MM&lt;/td&gt;
   &lt;td&gt;
    Minutes; leading zero for single-digit minutes.&lt;br /&gt;
    &lt;span class="small"&gt;Uppercase MM unlike CF &lt;code&gt;timeFormat&lt;/code&gt;&amp;#39;s mm to avoid conflict with months.&lt;/span&gt;&lt;/td&gt;
  &lt;/tr&gt;
  &lt;tr&gt;
   &lt;td&gt;
    s&lt;/td&gt;
   &lt;td&gt;
    Seconds; no leading zero for single-digit seconds.&lt;/td&gt;
  &lt;/tr&gt;
  &lt;tr class="altBg"&gt;
   &lt;td&gt;
    ss&lt;/td&gt;
   &lt;td&gt;
    Seconds; leading zero for single-digit seconds.&lt;/td&gt;
  &lt;/tr&gt;
  &lt;tr class="altBg"&gt;
   &lt;td&gt;
    t&lt;/td&gt;
   &lt;td&gt;
    Lowercase, single-character time marker string: &lt;em&gt;a&lt;/em&gt; or &lt;em&gt;p&lt;/em&gt;.&lt;br /&gt;
    &lt;span class="small"&gt;No equivalent in CF.&lt;/span&gt;&lt;/td&gt;
  &lt;/tr&gt;
  &lt;tr&gt;
   &lt;td&gt;
    tt&lt;/td&gt;
   &lt;td&gt;
    Lowercase, two-character time marker string: &lt;em&gt;am&lt;/em&gt; or &lt;em&gt;pm&lt;/em&gt;.&lt;br /&gt;
    &lt;span class="small"&gt;No equivalent in CF.&lt;/span&gt;&lt;/td&gt;
  &lt;/tr&gt;
  &lt;tr class="altBg"&gt;
   &lt;td&gt;
    T&lt;/td&gt;
   &lt;td&gt;
    Uppercase, single-character time marker string: &lt;em&gt;A&lt;/em&gt; or &lt;em&gt;P&lt;/em&gt;.&lt;br /&gt;
    &lt;span class="small"&gt;Uppercase T unlike CF&amp;#39;s t to allow for user-specified casing.&lt;/span&gt;&lt;/td&gt;
  &lt;/tr&gt;
  &lt;tr&gt;
   &lt;td&gt;
    TT&lt;/td&gt;
   &lt;td&gt;
    Uppercase, two-character time marker string: &lt;em&gt;AM&lt;/em&gt; or &lt;em&gt;PM&lt;/em&gt;.&lt;br /&gt;
    &lt;span class="small"&gt;Uppercase TT unlike CF&amp;#39;s tt to allow for user-specified casing.&lt;/span&gt;&lt;/td&gt;
  &lt;/tr&gt;
  &lt;tr class="altBg"&gt;
   &lt;td&gt;
    Z&lt;/td&gt;
   &lt;td&gt;
    US timezone abbreviation, e.g. &lt;em&gt;EST&lt;/em&gt; or &lt;em&gt;MDT&lt;/em&gt;. With non-US timezones or in the Opera browser, the GMT/UTC offset is returned, e.g. &lt;em&gt;GMT-0500&lt;/em&gt;&lt;br /&gt;
    &lt;span class="small"&gt;No equivalent in CF.&lt;/span&gt;&lt;/td&gt;
  &lt;/tr&gt;
  &lt;tr&gt;
   &lt;td&gt;
    o&lt;/td&gt;
   &lt;td&gt;
    GMT/UTC timezone offset, e.g. &lt;em&gt;-0500&lt;/em&gt; or &lt;em&gt;+0230&lt;/em&gt;.&lt;br /&gt;
    &lt;span class="small"&gt;No equivalent in CF.&lt;/span&gt;&lt;/td&gt;
  &lt;/tr&gt;
  &lt;tr class="altBg"&gt;
   &lt;td&gt;
    &amp;#39;&amp;hellip;&amp;#39; &lt;em&gt;or&lt;/em&gt; &amp;quot;&amp;hellip;&amp;quot;&lt;/td&gt;
   &lt;td&gt;
    Literal character sequence. Surrounding quotes are removed.&lt;br /&gt;
    &lt;span class="small"&gt;CF only supports single quotes for literal spans.&lt;/span&gt;&lt;/td&gt;
  &lt;/tr&gt;
 &lt;/tbody&gt;
